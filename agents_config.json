    "video_editor": {
      "name": "Podcast Video Editor",
      "role": "Video production specialist focused on podcast content optimization",
      "model": "gpt-4o",
      "system_prompt": "You are an expert video editor specializing in podcast content. Your primary responsibilities include: 1) Analyzing multi-camera footage to identify and cut to active speakers using AI facial detection and voice activity detection, 2) Creating engaging short-form content (60-120 seconds) from long-form episodes, 3) Implementing professional color grading and visual effects, 4) Adding dynamic overlays, lower thirds, and branding elements, 5) Optimizing video for different platforms (YouTube, TikTok, Instagram, etc.). Always maintain consistent branding and focus on viewer engagement.",
      "tools": [
        {
          "name": "video_analysis",
          "description": "Analyze video footage for speaker detection and optimal cutting points",
          "parameters": {
            "video_path": "string",
            "analysis_type": ["speaker_detection", "engagement_scoring", "optimal_cut_points"]
          }
        },
        {
          "name": "auto_cut",
          "description": "Automatically cut between camera angles based on speaker activity",
          "parameters": {
            "input_video": "string",
            "output_video": "string",
            "cutting_style": ["dynamic", "conservative", "aggressive"]
          }
        },
        {
          "name": "create_short",
          "description": "Generate short-form content from long-form podcast",
          "parameters": {
            "source_video": "string",
            "duration": "number",
            "focus_topic": "string",
            "platform": ["tiktok", "instagram", "youtube_shorts"]
          }
        },
        {
          "name": "add_overlays",
          "description": "Add text overlays, lower thirds, and visual elements",
          "parameters": {
            "video_path": "string",
            "overlay_config": "object"
          }
        }
      ],
      "workflows": {
        "episode_edit": ["video_analysis", "auto_cut", "add_overlays", "export_final"],
        "short_creation": [
          "video_analysis",
          "create_short",
          "add_captions",
          "optimize_for_platform"
        ]
      }
    },
