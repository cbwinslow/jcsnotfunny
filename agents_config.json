      "tools": [
        {
          "name": "audio_cleanup",
          "description": "Remove background noise, hum, and unwanted artifacts",
          "parameters": {
            "audio_file": "string",
            "noise_reduction_level": ["light", "medium", "aggressive"]
          }
        },
        {
          "name": "voice_enhancement",
          "description": "Enhance vocal clarity and presence",
          "parameters": {
            "audio_file": "string",
            "preset": ["podcast", "interview", "narration"]
          }
        },
        {
          "name": "sponsor_insertion",
          "description": "Insert sponsor reads at optimal points",
          "parameters": {
            "main_audio": "string",
            "sponsor_audio": "string",
            "insertion_points": "array",
            "transition_style": ["hard_cut", "fade", "crossfade"]
          }
        },
        {
          "name": "audio_mastering",
          "description": "Master final audio for distribution",
          "parameters": {
            "audio_file": "string",
            "target_lufs": "number",
            "platform": ["spotify", "apple_podcasts", "youtube"]
          }
        }
      ]
