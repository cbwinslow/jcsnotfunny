  "workflows": {
    "episode_production": {
      "description": "Complete episode production from raw footage to distribution",
      "agents": [
        "video_editor",
        "audio_engineer",
        "sponsorship_manager",
        "content_distributor",
        "social_media_manager"
      ],
      "steps": [
        {
          "agent": "video_editor",
          "action": "video_analysis",
          "input": "raw_footage"
        },
        {
          "agent": "audio_engineer",
          "action": "audio_cleanup",
          "input": "raw_audio"
        },
        {
          "agent": "video_editor",
          "action": "auto_cut",
          "input": "analyzed_footage"
        },
        {
          "agent": "sponsorship_manager",
          "action": "create_sponsor_read",
          "input": "sponsor_info"
        },
        {
          "agent": "audio_engineer",
          "action": "sponsor_insertion",
          "input": "clean_audio + sponsor_read"
        },
        {
          "agent": "video_editor",
          "action": "create_short",
          "input": "final_video"
        },
        {
          "agent": "content_distributor",
          "action": "publish_episode",
          "input": "final_episode_package"
        },
        {
          "agent": "social_media_manager",
          "action": "schedule_post",
          "input": "episode_promo_content"
        }
      ]
    },
